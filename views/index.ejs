
   
   <div  class="Todolist-body">
        <div class="date">
            <h1><%- date %>, Todo List </h1>
            <i class="fa-solid fa-clipboard-list"></i>
        </div>
        <div>
           <% newItems.forEach(list => { %>
            <div class="list-items">
                <li>
                    <input class="checkbox" type="checkbox">
                    <p>
                        <%= list %>
                    </p>
                </li>
            </div>
            <% }); %>
                
         
        </div>
        <form action="/" method="post">
    
            <div class="input-section">
                <input placeholder="" type="text" name="userInput" aria-autocomplete="none" required autofocus>
                <button class="btn" type="submit" name="button"><i class="fa-solid fa-plus"></i></button>
            </div>
        </form>
    </div>

